@using FavoriteBands.Models;
@model Band[]
<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css">
    <title>My Title</title>
</head>
<body>
    
    <h1>Bands</h1>
    <ol>
        @foreach(var band in Model)
        {
            <li><a href="/bands/details/@band.ID">@band.Name</a></li>
        }
    </ol>

</body>
</html>